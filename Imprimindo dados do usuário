package com.mycompany.aula01;

/**
 *
 * @author aluno.den
 */
public class Aula01 {
    public static void main(String[] args) {
        //Declarando variáveis
        String nome = "Marta";
        int idade = 30;
        double peso = 65.2;
        
        //Declaração de variáveis 
        final String CPF = "123.456.789-00";
        
                
       //Exibindo dados 
       //Contatenando com o sinal de "+"
        System.out.println("O nome do usuário é:" + nome);
        System.out.println("O CPF do usuário é:" + CPF);
        System.out.println("A idade do usuário é:" + idade + " anos");
        System.out.println("O peso do usuário é:" + peso + " quilos");

    }
}
 
